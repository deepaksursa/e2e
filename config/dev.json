{
    "baseUrl": "https://example-app-dev.com",
    "apiUrl": "https://api.example-app-dev.com/v1",
    "timeout": 30000,
    "retries": 2,
    "users": {
        "admin": {
            "username": "admin@example.com",
            "password": "DevPassword123!"
        },
        "customer": {
            "username": "customer@example.com",
            "password": "DevPassword123!"
        },
        "guest": {
            "username": "guest@example.com",
            "password": "DevPassword123!"
